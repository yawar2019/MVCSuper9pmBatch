@model List<MVCSuper9pmBatch.Models.Employee>
@{
    ViewBag.Title = "SendMultipleObject";
}

<h2>SendMultipleObject</h2>

<table class="table-condensed table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>